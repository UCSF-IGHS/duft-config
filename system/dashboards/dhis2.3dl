                            detailsViewColumn="client_id"
                        >
                            <Table data="new_detailed_details_data" />
                        </Table>
                    </Tile>
                    <Tile data="tile_new_data" title="New Cases" iconType="people">
                        <Table
                            data="new_detailed_data"
                            exportData="true"
                            header="New Cases"
                            variant="plain"
                            initialColumns="client_id,gender, hiv_diagnosis_date,enrolment_date"
                            detailsViewColumn="client_id"
                        >
                            <Table data="new_detailed_details_data" />
                        </Table>
                    </Tile>
                    <Tile data="tile_new_data" title="New Cases" iconType="question">
                        <Table
                            data="new_detailed_data"
                            exportData="true"
                            header="New Cases"
                            variant="plain"
                            initialColumns="client_id,gender, hiv_diagnosis_date,enrolment_date"
                            detailsViewColumn="client_id"
                        >
                            <Table data="new_detailed_details_data" />
                        </Table>
                    </Tile>
                    <Tile data="tile_new_data" title="New Cases" iconType="medicine">
                        <Table
                            data="new_detailed_data"
                            exportData="true"
                            header="New Cases"
                            variant="plain"
                            initialColumns="client_id,gender, hiv_diagnosis_date,enrolment_date"
                            detailsViewColumn="client_id"
                        >
                            <Table data="new_detailed_details_data" />
                        </Table>
                    </Tile>
                    <Tile data="tile_new_data" title="New Cases" iconType="alert">
                        <Table
                            data="new_detailed_data"
                            exportData="true"
                            header="New Cases"
                            variant="plain"
                            initialColumns="client_id,gender, hiv_diagnosis_date,enrolment_date"
                            detailsViewColumn="client_id"
                        >
                            <Table data="new_detailed_details_data" />
                        </Table>
                    </Tile>
            </Row>
        </Grid>
    </Dashboard>

</Dashboards>