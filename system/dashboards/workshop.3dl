<Dashboard>
	<Grid>
		<Row>
			<Header>Preview query</Header>
		</Row>

		<Row columns = "1" largeColumns = "3">

			<Dataset pageSize={50} sort_column={ s.name } searchColumns = "t.name" >
				<Header small="true">
					Available Database Tables
				</Header>
				<Query>
					<Include file="sql/table_list.sql" />
				</Query>
				<InfiniteScrollTable exportData="true" variant='plain' />
			</Dataset>
		</Row>
	</Grid>
</Dashboard>
