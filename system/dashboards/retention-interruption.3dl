<Dashboards>
  <Datasets>
    <SQLData
      pageSize="20"
      name="retention_interruption"
      queryName="retention.sql"
      filters={{ "Retention Status": 'Treatment Interruptions' }}
      searchColumns="ART Number Legacy"
    />

    <SQLData
      name="retention_interruption_details_data"
      detailsViewData="retention_interruption"
      detailsViewColumn="ART Number Legacy"
      transpose="true"
    >
      SELECT * from analysis.fact_sentinel_event WHERE "ART Number Legacy"='[[ART Number Legacy]]'
    </SQLData>
  </Datasets>
  <Grid>
    <SingleLayout>
      <Header>Treatment Interruptions</Header>
      <Table
        data="retention_interruption"
        exportData="true"
        variant="plain"
        detailsViewColumn="ART Number Legacy"
        initialColumns="Patient category, ART Number Legacy, Date Of Birth, Current Age, Sex, Pregnancy/BF status, Last Visit Date, Next Follow-up Date, Last VL order date, Last VL result date, Current ART Regimen, Current ART Regimen start date, ART Restart Date, Retention Status, Client Status"
      >
        <Table data="retention_interruption_details_data" />
      </Table>
    </SingleLayout>

  </Grid>
</Dashboards>
