				</Row>

				<Row columns="1" largeColumns="1" >
					<Dataset pageSize={50} searchColumns="gender, date_of_birth" >
						<Query>
							<Include file="sql/hei/not_registered.sql" />
						</Query>
						<Include file="components/client_list.3dl" />
					</Dataset>
				</Row>

				</Grid>
			</SingleLayout>
		</Dashboard>
	</Tab>

